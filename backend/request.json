# create a document
PUT pics 
{
  "settings":{
    "number_of_shards": 1,
    "number_of_replicas": 1
  },
  "mappings":{
    "properties": {
      "pic_id" :{
        "type" : "keyword"
        },
      "crd" :{
        "type": "dense_vector",
        "dims": 2048
        }
    }
  }
}

# edit a document , /pics/_mapping is the only entry usable
PUT /pics/_mapping
{
  "properties": {
    "pic_id": {
      "type": "keyword"
    },
    "dims": {
      "type": "dense_vector",
      "dims": 1000
    }
  }
}

